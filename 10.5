def bubble_sort(arr):
    n = len(arr)
    for i in range(n):
        for j in range(0, n-i-1):
            if arr[j] > arr[j+1]:
                arr[j], arr[j+1] = arr[j+1], arr[j]
    return arr

# Input
n = int(input())
arr = list(map(int, input().split()))

# Call the function to sort the list using bubble sort
sorted_arr = bubble_sort(arr)

# Output the sorted list
print(*sorted_arr)
